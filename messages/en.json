  "Services": {
    "title": "Service",
    "onlineAccounting": {
      "title": "Online Accounting",
      "description": "Service characteristics:",
      "details": [
        "Cloud Accounting for bookkeeping through standard software",
        "Sending trade documents via electronic media such as Email, Line, SMS, Fax, Scan, Photos",
        "Ability to view transactions via mobile (All Devices)",
        "Within 24 hours after sending documents, you can view and issue financial statements and drill down documents"
      ],
      "moreDetails": "More Details"
    },
    "payrollOutsourcing": {
      "title": "Payroll Outsourcing",
      "description": "Service characteristics:",
      "details": [
        "Outsourced payroll services, can divide salary payment into 4 installments",
        "Internet-based services for PND1 filing, social security filing, employee entry and exit notifications",
        "Preparation of files for bank submission",
        "Filing of PND90, 91 forms with tax calculation sheets for employees"
      ],
      "moreDetails": "More Details"
    },
    "bookkeepingServices": {
      "title": "Bookkeeping Services",
      "description": "Service characteristics:",
      "details": [
        "Tax Accounting: Providing accounting services in the form of an accounting office covering tax work, social security, Department of Business Development, handling related government filings, submitting annual financial statements",
        "Financial Accounting: Monthly accounting and closing, focusing on accuracy according to accounting standards, bank reconciliation, prioritizing the company's cash flow",
        "Management Accounting: Providing management accounting services, a continuation of Financial Accounting by presenting information to executives in the form of Dashboard, Infographic, PowerPoint"
      ],
      "moreDetails": "More Details"
    },
    "businessStartup": {
      "title": "Business Startup",
      "description": "Service characteristics:",
      "details": [
        "Company and legal entity establishment",
        "Preparation of forms for business use",
        "Basic business plan advice for new entrepreneurs",
        "Consultation on important trade laws for the business",
        "Handling changes to existing legal entities, capital increase, change of director authority, company name change, change of business objectives, VAT registration"
      ],
      "moreDetails": "More Details"
    }
